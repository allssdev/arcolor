	cTipo    := Subs(cTITPAI,(1+nTamPref+nTamNum+nTamParc),nTamTipo)   
	
	If cTipo $ MVRECANT // Titulo pai e um RA
		lRet := .F.
	EndIf	
EndIf

RestArea(aArea)

Return lRet