Next
If MV_PAR07 <> 1
	oSection2:Disable()
EndIf
_cS_MV08 := "%" + StrTran(_cS_MV08,"  "," ") + " " + _cField + "%"
_cJ_MV08 := "%" + StrTran(_cFilSPJ,"%","") + " " + StrTran(_cJ_MV08,"  "," ") + "%"
_cField  := "%" + _cField + "%"
