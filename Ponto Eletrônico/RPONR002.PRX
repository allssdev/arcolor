Local _cFilSRA := oSection:GetSqlExp("SRA")
Local _cFilSP8 := oSection:GetSqlExp("SP8")
Local _cFilSRJ := oSection:GetSqlExp("SRJ")
Local _cFilSR6 := oSection:GetSqlExp("SR6")
Local _cFilSPJ := oSection:GetSqlExp("SPJ")
